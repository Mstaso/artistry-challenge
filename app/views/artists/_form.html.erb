
<% if flash[:my_errors]%>
<% flash[:my_errors].each do |error|%>
<p><%= error %></p>
<%end%>
<%end%>




<%= form_for @artist do |f|%>
<%= f.label :name%><br>
<%= f.text_field :name%><br>
<%= f.label :title%><br>
<%= f.text_field :title%><br>
<%= f.label :age%><br>
<%= f.number_field :age%><br>
<%= f.collection_select :id, Instrument.all, :id, :name %><br>
<%= f.submit%>
<%end%>